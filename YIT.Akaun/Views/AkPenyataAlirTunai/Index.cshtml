                        <div class="form-actions no-color mb-3">
                            <div class="row">
                                <div class="col-md-2">
                                    <label class="form-control-label">Bank</label>
                                </div>
                                <div class="col-md-8">
                                    <div class="input-group mb-2 input-group-sm ">
                                        <select class="form-control form-control-sm select2" name="AkBankId" asp-items="@ViewBag.AkBank" required></select>
                                        <label class="invalid-feedback">Sila isi ruangan ini.</label>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    <div class="input-group mb-2 input-group-sm ">
                                        <label class="form-control-label">Kump. Wang</label>
                                    </div>
                                </div>
                                <div class="col-md-8">

                                    <div class="input-group mb-2 input-group-sm ">
                                        <select class="form-control form-control-sm select2" name="JKWId" asp-items="@ViewBag.JKW"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    <div class="input-group mb-2 input-group-sm ">
                                        <label class="form-control-label">PTJ</label>
                                    </div>
                                </div>
                                <div class="col-md-8">

                                    <div class="input-group mb-2 input-group-sm ">
                                        <select class="form-control form-control-sm select2" name="JPTJId" asp-items="@ViewBag.JPTJ"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-2">
                                    <div class="input-group mb-2 input-group-sm ">
                                        <label class="form-control-label">Tahun</label>
                                    </div>
                                </div>
                                <div class="col-md-4">

                                    <div class="input-group mb-2 input-group-sm ">
                                        <input class="form-control form-control-sm" type="text" name="Tahun1" value="@ViewData["Tahun1"]" required />
                                        <label class="invalid-feedback">Sila isi ruangan ini.</label>
                                    </div>
                                </div>
                            </div>
